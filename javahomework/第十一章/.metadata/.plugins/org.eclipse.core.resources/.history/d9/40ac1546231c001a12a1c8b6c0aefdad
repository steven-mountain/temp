package Person11_2;

import java.util.ArrayList;

public class Student extends Person{
	int sat;
	
	final String[] sta = {"大一", "大二", "大三", "大四"};
	
	protected ArrayList<String> state = new ArrayList<String>();
	
	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Student(String name, String address, String phoneNumber,String emailAddr, int sat) {
		super(name, address, phoneNumber, emailAddr);
		this.sat = sat;
		// TODO Auto-generated constructor stub
	}

	@Override
	public String toString() {
		return super.toString() + "Student [state=" + state + "]";
	}

	
}
