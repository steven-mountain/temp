package LargeNumber;

import java.math.BigInteger;

public class Test {
	public static void main(String[] args) {
		BigInteger l1 = new BigInteger("123465789");
		BigInteger l2 = new BigInteger("987654321");
		System.out.println("" + l1.add(l2));
		System.out.println("" + l1.divide(l2));
		System.out.println("" + l1.multiply(l2));
		System.out.println("" + l1.subtract(l2));
//		System.out.println("add:");
//		System.out.println("l1 + l2 :" + l1.add(l2).toString());
//		System.out.println("muti:");
//		System.out.println("l1 * l2 :" + l1.multiply(l2).toString());
//		System.out.println("sub");
//		System.out.println("l1 / l2 :" + l1.subtract(l2).toString());
//		System.out.println("divid");
//		System.out.println("l1 - l2 :" + l1.divide(l2).toString());
	}
}
